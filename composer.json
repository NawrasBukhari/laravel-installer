{
    "name": "nawrasbukhari/laravel-installer",
    "description": "Laravel web installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Nawras Bukhari",
            "email": "nawrasbukhari@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "7.*|8.*|9.*|10.*"
    },
    "autoload": {
        "psr-4": {
            "NawrasBukhari\\LaravelInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NawrasBukhari\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require-dev": {
        "laravel/pint": "^1.10"
    }
}
